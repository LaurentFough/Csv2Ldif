{
  "name": "csv2ldif",
  "version": "0.2.0",
  "author": "Nordes Menard-lmaarre",
  "license": "MIT",
  "description": "Generate the ldif from excel",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Nordes/Csv2Ldif.git"
  },
  "keywords": [
    "ldif",
    "OpenLdap",
    "converter"
  ],
  "scripts": {
    "build": "rimraf lib && tsc -p ./",
    "watch": "rimraf lib && tsc -p ./ -w",
    "ts-typings": "tsc --declaration ./src/index.ts",
    "test": "nyc mocha test/*.spec.ts"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "all": true
  },
  "dependencies": {
    "csvtojson": "^1.1.9",
    "yargs": "^11.0.0"
  },
  "devDependencies": {
    "@types/csvtojson": "^1.1.5",
    "@types/mocha": "^2.2.48",
    "@types/yargs": "^10.0.1",
    "mocha": "^5.0.0",
    "nyc": "^11.4.1",
    "rimraf": "^2.6.2",
    "typescript": "^2.7.1"
  }
}
